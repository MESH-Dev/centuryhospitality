/* Table of Content
==================================================
 

/* Header Styles
================================================== */
#header{
	background-color: #ffffff;
	height: auto;
	width: 100%;
	padding-top:20px; padding-bottom:10px;
}


.logo{
	padding-top: 20px;

	&:hover{@include opacity(0.8); }

	@include breakpoint(smallscreen) {
 		padding-top: 36px;
		img {max-width:175px; }
	}
	
	@include breakpoint(landscape) {
		padding-bottom:4px;
 		border-bottom: 1px solid $bluegray;
		img {   }
		&:after{content:""; 
			width:100%; 
			margin-top:4px; 
			display:block;
			border-bottom: 1px solid $bluegray; 
		}
	}
	@include breakpoint(portrait) {
		padding-bottom:4px;
 		border-bottom: 1px solid $bluegray;
		img {   }
		&:after{content:""; 
			width:100%; 
			margin-top:4px; 
			display:block;
			border-bottom: 1px solid $bluegray; 
		}
	}
}

.social{
	height: 30px;  
	span{margin-left:4px; float:right}
	span:hover{cursor: pointer; @include opacity(0.8);}

	.st_email_custom{
		background: url("../images/email.png") no-repeat scroll left top transparent;
		padding:29px 29px 0 0;
	}
	.st_facebook_custom{
		background: url("../images/fb.png") no-repeat scroll left top transparent;
		padding:29px 29px 0 0;
	}
	.st_twitter_custom{
		background: url("../images/twitter.png") no-repeat scroll left top transparent;
		padding:29px 29px 0 0;
	}
	.st_sharethis_custom{
		background: url("../images/share.png") no-repeat scroll left top transparent;
		padding:29px 29px 0 0;
	}
}

.navigation{  
	margin-top: 10px; 
	padding-bottom: 4px; 
	border-bottom: 1px solid $bluegray;

	@include breakpoint(landscape) { 
   		margin-top: 10px; 
		padding-bottom: 0px;  
    }

	@include breakpoint(portrait) { 
		margin-top: 10px; 
		padding-bottom: 0px; 
	}
}
.navigation:after{
	content:""; 
	width:100%; 
	margin-top:4px; 
	display:block;
	border-bottom: 1px solid $bluegray; 
	@include breakpoint(landscape) { 
   		 border-bottom: none; 
    } 
    @include breakpoint(portrait) { 
   		 border-bottom: none; 
    } 
}

ul#nav{
	height:20px;
	margin-bottom: 8px;
	margin-top: 24px;
	width: 100%; 	
	text-align: justify;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;

    @include breakpoint(smallscreen) {

	}

	@include breakpoint(landscape) {
		height: auto; text-align: left;
		margin-bottom: 0px; 
	}
	@include breakpoint(portrait) {
		height: auto; text-align: left;
		margin-bottom: 0px; 
	}


	li{
		font-family: "Montserrat", sans-serif;
		text-transform: uppercase; 
		font-weight:bold; 
		vertical-align: top;
    	display: inline-block;    
    	*display: inline;   
    	zoom: 1;
		a{text-decoration: none;}
		a:hover{text-decoration: none;  } 
		&:last-child{padding-right:0px;}

		@include breakpoint(smallscreen) {
    	 	font-size: 11px;	
		}

		@include breakpoint(landscape) {
			display: block; padding-top:5px;       
		}
		@include breakpoint(portrait) {
			display: block; padding-top:5px;       
		}


	}
	.stretch{width: 100%; display: inline-block; font-size: 0; line-height: 0;}
}


@include breakpoint(landscape) 
{
	.menu-main-navigation-container{ }
 	.navigation {  }
 
	 
}
/* Hompage Styles
================================================== */
 
.slides{ margin-bottom:0px;}
.rslides {
  position: relative;
  list-style: none;
  overflow: hidden;
  width: 100%;
  padding: 0;
  margin: 0;
  margin-top: 10px;

  li {
	  -webkit-backface-visibility: hidden;
	  position: absolute;
	  display: none;
	  width: 100%;
	  left: 0;
	  top: 0;
	  padding-bottom:0px;
  	  margin-bottom: 0px;
  }

  li:first-child {
	  position: relative;
	  display: block;
	  float: left;
	   }

	   img {
	  display: block;
	  height: auto;
	  float: left;
	  width: 100%;
	  border: 0;
   }
}

.banner{
	background-color: $navy;
	height: auto;
	width: 100%;
	margin-bottom:20px;
	.logos{
		text-align:center;
		padding-top:15px;
		padding-bottom:15px;

		img{padding-left:10px; padding-right:10px;}
		img:hover{@include opacity(0.8); }
	}
}

.border_box{ border: 1px solid $bluegray;   }
.border_inner{margin: 4px;	border: 1px solid $lightred;}


.home_quote {
	display: block;
	position: absolute;
	z-index: 2;
	font-size: 20px;
	text-shadow: none;
	color: $red;
	background: #fff;
	background: rgba(255,255,255, .70);
	left: 0;
	top: 5%;
	  
	@include columns(4);

	 p{padding: 12px 5px 12px 5px; border: 1px solid $gray; margin:5px; background: rgba(255,255,255, .70); font-size:20px; line-height:26px; font-family: "Montserrat", sans-serif; text-transform:uppercase; font-weight:bold; text-align: center;}	

	@include breakpoint(smallscreen) { 
		 
		@include columns(4, $sk-tablet-col);
	}
   
    @include breakpoint(landscape) { 
    	  
		@include columns(0);  
    	left: 0; 	
    	top: 4%; 
		  width: 420px;
    	 p{padding: 10px 10px 10px 10px; margin:5px; background: rgba(255,255,255, .70); font-size:16px; line-height:20px; font-family: "Montserrat", sans-serif;  text-transform:uppercase; font-weight:bold;}	
    }

	@include breakpoint(portrait) {  
    	width: 300px;
    	 
		@include columns(0);  
    	left: 0; 	 
		top: 2%; 
    	 p{padding: 10px 10px 10px 10px; margin:5px; background: rgba(255,255,255, .70); font-size:16px; line-height:20px; font-family: "Montserrat", sans-serif; text-transform:uppercase; font-weight:bold;}	
	}

}

.callout{	@include breakpoint(landscape) { 
     	margin-top: 20px;
    }

	@include breakpoint(portrait) { 
	 	margin-top: 20px;
	}}

.home_callout{
	 
	margin:4px;
	border: 1px solid $lightred;
	text-align: center; 
	padding: 16px 0px 16px 0px; 

	@include breakpoint(smallscreen) { 
		 height:82px;
	}

	h2{
		font-size:28px; 
		line-height:36px;
		text-transform: uppercase;
		font-weight: bold;

		a{
			color: $darkgray;
			text-decoration: none;
		}
		a:hover{
			color: $red;
			text-decoration: none;
		}
		
    }
} 

.home_line{  
	margin-top: 30px; 
	padding-bottom: 4px; 
	border-bottom: 1px solid $bluegray;

	&:after{
		content:""; 
		width:100%; 
		margin-top:4px; 
		display:block;
		border-bottom: 1px solid $bluegray  
	}
}

.home_column{
	padding: 15px 0px 10px 0px;

	h2{
		text-transform: uppercase; 
		font-weight: normal; 
		border-bottom: 1px solid $bluegray;
		padding-bottom: 4px;
		color: $navy;
		&:after{
			content:""; 
			width:100%; 
			 margin-top: 12px; 
			display:block;
			border-bottom: 1px solid $bluegray  
		}
	}

	p{
		padding-right:30px;
	}
	a.more{ padding-top:16px;}
 

}

a.more{float:right; color: $bluegray; font-weight:bold; font-style: italic;}
a.more:hover{color: $red}

/* SUBHEADER Styles
================================================== */
 .page_title {
	display: block;
	position: absolute;
	z-index: 2;
	font-size: 20px;
	text-shadow: none;
	color: $red;
	background: #fff;
	background: rgba(255,255,255, .70);
	left: 7px;
	right: 0;
	bottom: 8%;
	@include margin-offset(7); 
	@include columns(5);

	 h1{padding: 20px 12px 20px 15px; margin:5px; background: rgba(255,255,255, .70); font-family: "Montserrat", sans-serif;  text-transform: uppercase; text-align: center; color: $red;}	

	@include breakpoint(smallscreen) { 
		@include margin-offset(7, $sk-tablet-col);
		@include columns(5, $sk-tablet-col);
	}
   
    @include breakpoint(landscape) { 
    	width: 410px;
    	@include margin-offset(0); 
		@include columns(0);  
    	right: 0; 	 
		bottom: 4%; 
    	h1{padding: 10px 5px 5px 10px; margin:5px; background: rgba(255,255,255, .70); font-size:18px; line-height:20px; font-family: "Montserrat", sans-serif;   }	
    }

	@include breakpoint(portrait) { 
		width: 300px;
    	@include margin-offset(0); 
		@include columns(0);  
    	left: 0; 	 
		bottom: 2%; 
    	h1{padding: 8px 5px 8px 5px; margin:5px; background: rgba(255,255,255, .70); font-size:18px; line-height:20px; font-family: "Montserrat", sans-serif; }	
	}

}

/* PAGE Styles
================================================== */
.page_content{
	margin-top: 20px;
	 
	h2{
		text-transform: uppercase; 
		color: $red;
		font-weight: normal; 
		border-bottom: 1px solid $bluegray;
		padding-bottom: 4px;
		&:after{
			content:""; 
			width:100%; 
			margin-top: 0px; 
			display:block;
			border-bottom: 1px solid $bluegray ;
			 
		}
	}
}
.page_nav { 
	padding: 15px 15px 40px 15px;
	font-family: "Montserrat", sans-serif;
	li{padding-bottom: 6px;}
	li a{color: $darkgray; text-transform: uppercase; font-weight: bold;}
	li a:hover{color: $red;}
	@include breakpoint(landscape) {
		padding: 15px 15px 15px 15px;       
	}
	@include breakpoint(portrait) {
		padding: 15px 15px 15px 15px;       
	}
}

.page_side{margin-top: 20px;}


.testimonials{
	
	p{
		color: $red;
		font-family: "Montserrat", sans-serif; 
		font-weight:bold;
		font-style: italic;
		padding: 25px 15px 0px 15px;
	}

	p.quote_source{
		padding-top: 16px;
		font-weight: normal;
		padding: 0px 15px 25px 15px;
	}

	@include breakpoint(landscape) { 
    	 	
    }

	@include breakpoint(portrait) { 
		 display: none;	
	}
}

.landing_blocks{
	@include column-offset-no-gutter(1);
	.columns {margin-bottom:20px;}

	@include breakpoint(smallscreen) { 
		 padding-left: 20px;
	}
   
    @include breakpoint(landscape) { 
     	padding-left: 0px;
    }

	@include breakpoint(portrait) { 
	 	padding-left: 0px;
	}
}

.content{

	ul, ol { margin-bottom: 10px; }
	ul { list-style: outside; }
	ol { list-style: decimal; }
	ol, ul { margin-left: 30px; }
 
	ul ul, ul ol,
	ol ol, ol ul { margin: 0px 0 0px 0px; font-size: 100%;  }
	ul ul li, ul ol li,
	ol ol li, ol ul li { margin-bottom: 0px; }
	li { line-height: 18px; margin-bottom: 0px; }

	@include breakpoint(landscape) { 
     	padding-top: 20px;
    }

	@include breakpoint(portrait) { 
	 	padding-top: 20px;
	}
}

/* JOBS PAGE Styles
================================================== */

.job_description{padding: 0px 0px 10px 0px; display:none;}
.job_title{
	padding: 0px 0px 10px 0px;
	&:hover{color: $red; cursor: pointer;}
}

a.applynow{font-weight: bold;  color: $red;
	&:hover{color: $navy; }
  }

/* FOOTER Styles
================================================== */
#footer{
	width: 100%; 
	background-color: $navy;
	margin-top: 80px;
}
.footer{
	padding-top:20px;
	color: #ffffff;
	.menu {

		ul{
			-moz-column-count: 3;
		    -moz-column-gap: 20px;
		    -webkit-column-count: 3;
		    -webkit-column-gap: 20px;
		    column-count: 3;
		    column-gap: 20px;
		    width: 450px;

		    @include breakpoint(portrait) {
		 		display:none;
			}
			@include breakpoint(landscape) {
		 		display:none;
			}
		}
		li{
			list-style-type: none; 
			font-weight:bold;
			 margin-bottom:18px;  

			ul li {font-weight: normal;margin-bottom:4px; }
	    }
		li a{
			color: #fff;
			&:hover{color: $gray}
		}
	}

	h3{text-transform: uppercase; font-weight:bold; color: #ffffff;font-size:18px; margin-bottom:18px; }
	span.contact{color: #ffffff;}
	p {color: #ffffff;}
	p a{color:#ffffff}
	p a:hover{color:$gray;}
	a img:hover{@include opacity(0.7); }

	.footer_icon{
		padding-top:125px;
		@include breakpoint(smallscreen) {
 		padding-top: 180px;
		img {max-width:50px; }
		}

		@include breakpoint(landscape) { 
     		padding-top: 10px;
     		img {display:none; }
	    }

		@include breakpoint(portrait) { 
		 	padding-top: 10px;
		 	img {display:none; }
		}

	}
}

.copyright{
	border-top:1px solid $bluegray; 
	padding-top:10px;
	

}
.mesh{
	float:right; margin-top:-38px;
 

		@include breakpoint(landscape) { 
     		margin-top:0px; float:left;
	    }

		@include breakpoint(portrait) { 
		 	margin-top:0px; float:left;
		}

}

